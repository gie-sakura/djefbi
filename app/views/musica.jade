      //page header
      section(id="page-header")
        div(class="container clearfix")
          hgroup
            h1(class="page-title") Lazarus
            h2(class="page-subtitle") New album by DJ Envato.
          //share it
          div(id="share-wrap")
            div(id="share" data-url="release2.html" data-text="Single Release" data-title="share")
          hr
      section(id="main-content" class="container clearfix")
        //left
        article(class="col-1-3")
          h2(class="heading-icon deck") Lazarus album
            span(class="color") Dj Envato.
          ul(class="details none")
            li
              span Date
              div 2012-09-03
            li
              span Label
              div Eprom
            li
              span Catalog
              div EPROM22
            li
              span Genres
              div
                a(href="#") UK Funky
          //clear fix
          div(class="clear")
          p
            img(src="placeholders/release-01-small-cover.png" alt="Release cover" class="size-3-4")
          //single navigation
          div(id="single-nav")
            a(href="#" class="nav-prev")
            a(href="#" class="nav-next")
        //right
        article(class="col-2-3 last")
          h3 Tracklist.
          ol(class="playlist")
            li
              div(class="track" data-url="placeholders/mp3/adg3com_electrofreak.mp3") ADG3 Studios - Electro Freak
              div(class="playlist-details") Sepcial quest: DJ KODE88
              a(href="#" class="playlist-button") Buy
            li
              div(class="track" data-url="placeholders/mp3/adg3com_chuckedknuckles.mp3") ADG3 Studios - Chucked Nuckles
