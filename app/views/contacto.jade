            a(href="#" class="social-icon facebook")
            a(href="#" class="social-icon lastfm")
            a(href="#" class="social-icon soundcloud")
            a(href="#" class="social-icon myspace")
            a(href="#" class="social-icon youtube")
    //footer
    footer(id="footer" class="clearfix")
